{
  "name": "core-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=16 <17"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "BABEL_ENV=webpack webpack serve --mode=development",
    "dev:tsc": "yarn dev & yarn tsc --watch --noEmit",
    "dev:be": "NODE_ENV=development nodemon --watch './src/server/**/*' --ext 'ts,js' --exec ts-node -r ./node_modules/tsconfig-paths/register -- $NODE_DEBUG_OPTION --files ./src/server/index.ts",
    "dev:be:tsc": "yarn dev:be & yarn tsc --watch --noEmit",
    "build:be": "BABEL_ENV=production NODE_ENV=production webpack --config webpack-server.config.babel.js --mode=production",
    "build:be:analyze": "ANALYZE=true yarn build:be",
    "start": "NODE_ENV=production node distServer/index.js",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:quiet": "eslint src --ext js,jsx,ts,tsx --quiet",
    "lint:quiet:watch": "nodemon --watch './src/**/*' --exec yarn lint:ts:quiet",
    "lint:fix": "yarn lint --fix",
    "lint:ts:fix": "yarn lint:ts --fix",
    "test": "jest ./src --verbose",
    "test:coverage": "jest ./src --coverage  --verbose",
    "test:watch": "jest ./src --watch  --verbose",
    "tsc": "tsc",
    "prepush": "yarn lint && yarn test && yarn tsc"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/connect-timeout": "^0.0.36",
    "@types/cors": "^2.8.13",
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "firebase": "^9.15.0",
    "firebase-admin": "^11.4.1",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@earlorg/eslint-config": "^1.5.24",
    "@sentry/node": "^7.28.1",
    "@types/body-parser": "^1.19.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.27.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
